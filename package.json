{
  "name": "get-video-id",
  "version": "3.4.0",
  "description": "Get the YouTube, Vimeo, Vine, Microsoft Steam, Dailymotion, TikTok or VideoPress video id from a url or embed string.",
  "license": "MIT",
  "repository": "radiovisual/get-video-id",
  "main": "dist/get-video-id.js",
  "module": "dist/get-video-id.esm.js",
  "types": "./index.d.ts",
  "author": {
    "name": "Michael Wuergler",
    "email": "wuergler@gmail.com",
    "url": "https://github.com/radiovisual"
  },
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "pretest": "npm run build",
    "test": "npm run lint && jest --verbose && nyc --exclude dist jest --coverage",
    "lint": "xo",
    "lint:fix": "xo --fix",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "build": "rimraf dist && rollup -c rollup.config.js"
  },
  "files": [
    "index.js",
    "dist/*",
    "index.d.ts"
  ],
  "keywords": [
    "RegExp",
    "regexp",
    "iframe",
    "embed",
    "url",
    "vine",
    "vimeo",
    "youtube",
    "videopress",
    "microsoft stream",
    "microsoft",
    "dailymotion",
    "tiktok",
    "stream",
    "from",
    "string",
    "id",
    "video",
    "read",
    "retrieve",
    "fetch",
    "parse",
    "extract",
    "get"
  ],
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/eslint-parser": "^7.12.1",
    "@babel/plugin-transform-modules-commonjs": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.1.0",
    "coveralls": "^2.13.3",
    "jest": "^27.1.0",
    "nyc": "^6.1.1",
    "rimraf": "^3.0.0",
    "rollup": "^1.20.3",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-cleaner": "^1.0.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.1.1",
    "xo": "^0.44.0"
  },
  "xo": {
    "globals": [
      "test",
      "expect",
      "describe"
    ]
  }
}